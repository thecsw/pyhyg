import math

app_mag = [11.010,
           10.270,
           2.740,
           5.080,
           1.160,
           10.360,
           12.370,
           11.500]
dist = [1.2959,
        8.0567,
        11.6850,
        10.1978,
        10.3584,
        15.9719,
        4.2626,
        4.6081]
glisid = [551,
          623,
          "482A",
          231,
          286,
          841,
          35,
          440]


for i in range(len(app_mag)):
    avm = app_mag[i] - 5 * (math.log(dist[i], 10) - 1)
    print("{} & {} & {}\\\\".format(glisid[i], avm, round(avm, 3)))
